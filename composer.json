{
    "name": "olssonm/livewire-synthesizer",
    "type": "library",
    "require": {
        "livewire/livewire": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.8",
        "pestphp/pest": "^2.28"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Olssonm\\LivewireSynthesizer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Marcus Olsson"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
